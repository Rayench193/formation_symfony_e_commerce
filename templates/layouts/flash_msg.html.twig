{% for type, messages in app.flashes(['success','danger','inofo'])%}
    {%for message in messages%}
        <div class="col-md-12 max-auto mt-2" >
            <div class="alert  alert-{{type}} alert-dismissible fade show">
                {{message}}
            </div>
        </div>
    {%endfor%}
{%endfor%}